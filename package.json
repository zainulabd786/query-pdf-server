{
  "name": "query-pdf-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/bin/www",
    "dev": "cross-env NODE_ENV=dev npm-run-all build server",
    "prod": "cross-env NODE_ENV=prod npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@huggingface/inference": "^2.6.4",
    "@qdrant/js-client-rest": "^1.7.0",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "langchain": "^0.0.209",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.23.0",
    "rimraf": "^5.0.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5"
  }
}
